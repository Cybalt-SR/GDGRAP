      operation 130562 "RegisterActionListener"
	cpp_inline public explicit_return_type "void"
	nparams 2
	  param in name "name" explicit_type "std::string"
	  param inout name "action" type class_ref 129026 // ActionImplementation_base
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${typeprefix}${type} ${name}${(}${t0} ${p0}${v0}, ${t1} * ${p1}${v1}${)}${const}${volatile}${throw}${abstract} {
				if (player_actions.find(name) == player_actions.end())
					player_actions.insert_or_assign(name, std::list<ActionImplementation_base*>());

				player_actions[name].push_back(action);
			};
"
	
	
	
	
      end

      operation 130690 "UpdateStates"
	cpp_inline public explicit_return_type "void"
	nparams 2
	  param in name "callback" explicit_type "std::function<void(std::string, InputAction*, bool)>"
	  param inout name "target" explicit_type "Window"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${typeprefix}${type} ${name}${(}${t0} ${p0}${v0}, ${t1} * ${p1}${v1}${)}${const}${volatile}${throw}${abstract} {
				for (auto player_action : player_actions)
					for (auto action_implementation : player_action.second) {
						bool changed = action_implementation->CheckStateChanged(target);

						callback(player_action.first, action_implementation->GetState(), changed);
					}
			};
"
	
	
	
	
      end

      operation 130818 "ResetStates"
	cpp_inline public explicit_return_type "void"
	nparams 1
	  param inout name "target" explicit_type "Window"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${typeprefix}${type} ${name}${(}${t0} * ${p0}${v0}${)}${const}${volatile}${throw}${abstract} {
				for (auto player_action : player_actions)
					for (auto action_implementation : player_action.second) {
						action_implementation->ResetState(target);
					}
			};
"
	
	
	
	
      end
    end
