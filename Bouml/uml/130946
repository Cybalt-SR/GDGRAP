      operation 142338 "GetRandomValue"
	cpp_virtual cpp_override cpp_inline protected return_type class_ref 128386 // Vector3
	nparams 0
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${typeprefix}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract} {
				auto deltavec = this->valueB - this->valueA;
				auto randomvec = this->valueA;

				auto getrandf = []() {
					return (float)rand() / RAND_MAX;
				};

				randomvec.x += getrandf() * deltavec.x;
				randomvec.y += getrandf() * deltavec.y;
				randomvec.z += getrandf() * deltavec.z;

				return randomvec;
			};
"
	
	
	
	
      end
    end
