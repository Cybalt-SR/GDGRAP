      operation 147202 "TryCompileShader"
	class_operation public return_type class_ref 137986 // GLuint
	nparams 2
	  param in name "path" explicit_type "std::string"
	  param in name "compile_type" explicit_type "int"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${typeprefix}${type} ${name}${(}${t0} ${p0}${v0}, ${t1} ${p1}${v1}${)}${const}${volatile}${throw}${abstract};
"
	
	
	
	
	comment "/ <summary>
