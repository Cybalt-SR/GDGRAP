!!!142850.cpp!!!	GetViewMat() : glm::mat4

        auto world = this->World();

        auto this_pos = (glm::vec3)world->position;
        return glm::lookAt(this_pos, (glm::vec3)world->Forward + this_pos, (glm::vec3)world->Up);
